<script type="text/javascript">
function show_detail(lineNumber) {
    var detail = document.getElementById("detail_"+lineNumber);
    if(detail) {
        detail.className=(detail.className=='detail_hidden') ? 'detail_unhidden' : 'detail_hidden';
    }
}
</script>
